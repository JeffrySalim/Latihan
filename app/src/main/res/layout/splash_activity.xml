<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tool="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#18FFFF"
    tool:context=".SplashScreen">

    <View
        android:id="@+id/kotakHitam"
        android:layout_width="5dp"
        android:layout_height="5dp"
        android:layout_centerInParent="true"/>

    <ImageView
        android:id="@+id/kotakBiru"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_above="@id/kotakHitam"
        android:layout_centerHorizontal="true"
        android:src="@drawable/logocamp1"/>

    <ImageView
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/kotakHitam"
        android:layout_centerHorizontal="true"
        android:src="@drawable/logodevcamp"/>

</RelativeLayout>